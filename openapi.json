{
  "openapi": "3.0.0",
  "info": {
    "title": "票券申報API V1",
    "description": "票券申報API",
    "version": "v1"
  },
  "servers": [
    {
      "url": "http://localhost:8081/v1",
      "description": "本機開發環境"
    },
    {
      "url": "https://suptech-t.tdcc.com.tw/api/v1",
      "description": "業演環境"
    },
    {
      "url": "https://suptech.tdcc.com.tw/api/v1",
      "description": "正式環境"
    }
  ],
  "paths": {
    "/oauth/token": {
      "post": {
        "summary": "取得存取碼",
        "parameters": [
          {
            "$ref": "#/components/parameters/header_Xuser_id"
          },
          {
            "$ref": "#/components/parameters/header_Xinst_id"
          },
          {
            "$ref": "#/components/parameters/header_TxnInitDateTime"
          },
          {
            "$ref": "#/components/parameters/header_Xclient_type"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "grant_types": {
                    "type": "string",
                    "example": "client_credentites"
                  }
                },
                "required": [
                  "grant_types"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "認證成功後回傳 存取碼 及 存取種類 及 有效時間 等資訊",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "access_token": {
                      "type": "string",
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzY29wZSI6WyJkZWNsIl0sImV4cCI6MTU4ODA0MTg5NSwianRpIjoiYmE2NzE0NmYtYzQ4ZS00Yzc5LTkwYjctZmY1YzY3YmM2YmVlIiwiY2xpZW50X2lkIjoiamF2YWludXNlLWNsaWVudCJ9.dUaXdjRNKSpvj3k0qtAP1IWUDocAOxRkSNd4ptDa_FM"
                    },
                    "token_type": {
                      "type": "string",
                      "example": "bearer"
                    },
                    "expires_in": {
                      "type": "integer",
                      "example": "3600"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "5XX": {
            "$ref": "#/components/responses/UnexpectedError"
          }
        }
      }
    },
    "/oauth/check_token": {
      "post": {
        "security": [
          {
            "oAuth2ClientCredentials": []
          }
        ],
        "summary": "檢查存取碼",
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "核發的 access_token"
          },
          {
            "$ref": "#/components/parameters/header_Xuser_id"
          },
          {
            "$ref": "#/components/parameters/header_Xinst_id"
          },
          {
            "$ref": "#/components/parameters/header_TxnInitDateTime"
          },
          {
            "$ref": "#/components/parameters/header_Xclient_type"
          }
        ],
        "responses": {
          "200": {
            "description": "認證成功後回傳 token 效期及效力等資訊",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "active": {
                      "type": "boolean",
                      "example": true
                    },
                    "exp": {
                      "type": "integer",
                      "example": 1588247476
                    },
                    "jti": {
                      "type": "string",
                      "example": "9664d93e-06f4-4c32-ada3-cc586ca3554b"
                    },
                    "client_id": {
                      "type": "string",
                      "example": "0991234567"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "5XX": {
            "$ref": "#/components/responses/UnexpectedError"
          }
        }
      }
    },
    "/oauth/revocation": {
      "post": {
        "security": [
          {
            "oAuth2ClientCredentials": []
          }
        ],
        "summary": "撤銷存取碼",
        "parameters": [
          {
            "$ref": "#/components/parameters/header_Xuser_id"
          },
          {
            "$ref": "#/components/parameters/header_Xinst_id"
          },
          {
            "$ref": "#/components/parameters/header_TxnInitDateTime"
          },
          {
            "$ref": "#/components/parameters/header_Xclient_type"
          }
        ],
        "responses": {
          "200": {
            "description": "成功後回傳完成訊息",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "example": "token_revoke_success"
                    },
                    "message": {
                      "type": "string",
                      "example": "access token revoke success"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "5XX": {
            "$ref": "#/components/responses/UnexpectedError"
          }
        }
      }
    },
    "/decl/forms/status/insts/{inst_code}": {
      "get": {
        "security": [
          {
            "oAuth2ClientCredentials": []
          }
        ],
        "summary": "查詢報表狀態",
        "parameters": [
          {
            "$ref": "#/components/parameters/header_Xuser_id"
          },
          {
            "$ref": "#/components/parameters/header_Xinst_id"
          },
          {
            "$ref": "#/components/parameters/header_TxnInitDateTime"
          },
          {
            "$ref": "#/components/parameters/header_Xclient_type"
          },
          {
            "$ref": "#/components/parameters/path_inst_code"
          },
          {
            "$ref": "#/components/parameters/query_rpt_code"
          },
          {
            "$ref": "#/components/parameters/query_rpt_cycle"
          },
          {
            "$ref": "#/components/parameters/query_rpt_year"
          },
          {
            "$ref": "#/components/parameters/query_rpt_num"
          },
          {
            "$ref": "#/components/parameters/offsetParam"
          },
          {
            "$ref": "#/components/parameters/limitParam"
          }
        ],
        "responses": {
          "200": {
            "description": "資料傳遞內容透過 JWE 加密格式封裝, 未加密的PAYLOAD資料模型請參考 : [查詢報表申報狀態的查詢結果 : /decl/form/status : GET_RESPONSE_PAYLOAD]",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/JWE_RESPONSE"
                }
              }
            }
          },
          "201": {
            "$ref": "#/components/responses/Created"
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "5XX": {
            "$ref": "#/components/responses/UnexpectedError"
          }
        }
      }
    },
    "/decl/forms/{rpt_code}/years/{rpt_year}/nums/{rpt_num}/insts/{inst_code}": {
      "get": {
        "security": [
          {
            "oAuth2ClientCredentials": []
          }
        ],
        "summary": "查詢報表申報資料",
        "parameters": [
          {
            "$ref": "#/components/parameters/header_Xuser_id"
          },
          {
            "$ref": "#/components/parameters/header_Xinst_id"
          },
          {
            "$ref": "#/components/parameters/header_TxnInitDateTime"
          },
          {
            "$ref": "#/components/parameters/header_Xclient_type"
          },
          {
            "$ref": "#/components/parameters/path_rpt_code"
          },
          {
            "$ref": "#/components/parameters/path_rpt_year"
          },
          {
            "$ref": "#/components/parameters/path_rpt_num"
          },
          {
            "$ref": "#/components/parameters/path_inst_code"
          }
        ],
        "responses": {
          "200": {
            "description": "資料傳遞內容透過 JWE 加密格式封裝, 未加密的PAYLOAD資料模型請參考 : [查詢報表資料的查詢結果 : GET_RESPONSE_PAYLOAD]",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/JWE_RESPONSE"
                }
              }
            }
          },
          "201": {
            "$ref": "#/components/responses/Created"
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "5XX": {
            "$ref": "#/components/responses/UnexpectedError"
          }
        }
      },
      "post": {
        "security": [
          {
            "oAuth2ClientCredentials": []
          }
        ],
        "summary": "上傳報表申報資料",
        "parameters": [
          {
            "$ref": "#/components/parameters/header_Xuser_id"
          },
          {
            "$ref": "#/components/parameters/header_Xinst_id"
          },
          {
            "$ref": "#/components/parameters/header_TxnInitDateTime"
          },
          {
            "$ref": "#/components/parameters/header_Xclient_type"
          },
          {
            "$ref": "#/components/parameters/path_rpt_code"
          },
          {
            "$ref": "#/components/parameters/path_rpt_year"
          },
          {
            "$ref": "#/components/parameters/path_rpt_num"
          },
          {
            "$ref": "#/components/parameters/path_inst_code"
          }
        ],
        "requestBody": {
          "description": "資料傳遞內容透過 JWE 加密格式封裝, 未加密的PAYLOAD資料模型請參考 : [上傳報表資料的需求內容 : POST_REQUEST_PAYLOAD]",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/JWE_REQUEST"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "資料傳遞內容透過 JWE 加密格式封裝, 未加密的PAYLOAD資料模型請參考 : [上傳報表資料的處理結果 : POST_RESPONSE_PAYLOAD]",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/JWE_RESPONSE"
                }
              }
            }
          },
          "201": {
            "$ref": "#/components/responses/Created"
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "5XX": {
            "$ref": "#/components/responses/UnexpectedError"
          }
        }
      }
    },
    "/meta/forms/{rpt_code}": {
      "get": {
        "security": [
          {
            "oAuth2ClientCredentials": []
          }
        ],
        "summary": "查詢報表欄位定義",
        "parameters": [
          {
            "$ref": "#/components/parameters/header_Xuser_id"
          },
          {
            "$ref": "#/components/parameters/header_Xinst_id"
          },
          {
            "$ref": "#/components/parameters/header_TxnInitDateTime"
          },
          {
            "$ref": "#/components/parameters/header_Xclient_type"
          },
          {
            "$ref": "#/components/parameters/path_rpt_code"
          },
          {
            "$ref": "#/components/parameters/query_rpt_year"
          },
          {
            "$ref": "#/components/parameters/query_rpt_num"
          }
        ],
        "responses": {
          "200": {
            "description": "資料傳遞內容透過 JWE 加密格式封裝, 未加密的PAYLOAD資料模型請參考 : [查詢報表欄位定義的查詢結果 : GET_REQUEST_PAYLOAD]",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/JWE_RESPONSE"
                }
              }
            }
          },
          "201": {
            "$ref": "#/components/responses/Created"
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "5XX": {
            "$ref": "#/components/responses/UnexpectedError"
          }
        }
      }
    }
  },
  "components": {
    "parameters": {
      "header_Xuser_id": {
        "in": "header",
        "name": "X-UserId",
        "schema": {
          "type": "string"
        },
        "required": true,
        "description": "Client 使用者代號"
      },
      "header_Xinst_id": {
        "in": "header",
        "name": "X-InstId",
        "schema": {
          "type": "string"
        },
        "required": true,
        "description": "Client 使用者機構代號"
      },
      "header_TxnInitDateTime": {
        "in": "header",
        "name": "X-TxnInitDateTime",
        "schema": {
          "type": "string"
        },
        "required": true,
        "description": "交易發起日期時間 yyyyMMdd'T'HHmmssSSS'Z'"
      },
      "header_Xclient_type": {
        "in": "header",
        "name": "X-ClientType",
        "schema": {
          "type": "string",
          "enum": [
            "PCC"
          ]
        },
        "required": false,
        "description": "Client 種類"
      },
      "path_rpt_code": {
        "in": "path",
        "name": "rpt_code",
        "schema": {
          "type": "string",
          "enum": [
            "apici002",
            "apici201",
            "apici210",
            "apici215",
            "apici216",
            "apici217",
            "apici225",
            "apici226",
            "apici240",
            "apici281",
            "apici302",
            "apici303",
            "apici304",
            "apici320",
            "apici330",
            "apici343",
            "apici345",
            "apici395",
            "apici405",
            "apici407",
            "apici410",
            "apici415",
            "apici420",
            "apici425",
            "apici430",
            "apici450",
            "apici451",
            "apici601",
            "apici625",
            "apici701",
            "apici705",
            "apici901"
          ]
        },
        "required": true,
        "description": "報表代號"
      },
      "path_rpt_year": {
        "in": "path",
        "name": "rpt_year",
        "schema": {
          "type": "integer"
        },
        "required": true,
        "description": "申報年度"
      },
      "path_rpt_num": {
        "in": "path",
        "name": "rpt_num",
        "schema": {
          "type": "string",
          "enum": [
            "001",
            "002",
            "003",
            "004",
            "005",
            "006",
            "007",
            "008",
            "009",
            "010",
            "011",
            "012"
          ]
        },
        "required": true,
        "description": "申報期數"
      },
      "path_inst_code": {
        "in": "path",
        "name": "inst_code",
        "schema": {
          "type": "string"
        },
        "required": true,
        "description": "機構代碼"
      },
      "query_rpt_code": {
        "in": "query",
        "name": "rpt_code",
        "schema": {
          "type": "string",
          "enum": [
            "APICI002",
            "APICI201",
            "APICI210",
            "APICI215",
            "APICI216",
            "APICI217",
            "APICI225",
            "APICI226",
            "APICI240",
            "APICI281",
            "APICI302",
            "APICI303",
            "APICI304",
            "APICI320",
            "APICI330",
            "APICI343",
            "APICI345",
            "APICI395",
            "APICI405",
            "APICI407",
            "APICI410",
            "APICI415",
            "APICI420",
            "APICI425",
            "APICI430",
            "APICI450",
            "APICI451",
            "APICI601",
            "APICI625",
            "APICI701",
            "APICI705",
            "APICI901"
          ]
        },
        "description": "報表代碼",
        "example": "APICI002"
      },
      "query_rpt_cycle": {
        "in": "query",
        "name": "rpt_cycle",
        "schema": {
          "type": "string",
          "enum": [
            "M",
            "Q",
            "H",
            "Y",
            "O"
          ]
        },
        "description": "報表周期"
      },
      "query_rpt_year": {
        "in": "query",
        "name": "rpt_year",
        "schema": {
          "type": "integer"
        },
        "description": "申報年度"
      },
      "query_rpt_num": {
        "in": "query",
        "name": "rpt_num",
        "schema": {
          "type": "string",
          "enum": [
            "001",
            "002",
            "003",
            "004",
            "005",
            "006",
            "007",
            "008",
            "009",
            "010",
            "011",
            "012"
          ]
        },
        "description": "申報期數 (必須與申報年度合併使用)"
      },
      "offsetParam": {
        "in": "query",
        "name": "offset",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 0
        },
        "description": "分頁參數 - 位移量 : 這參數表示要跳過幾筆資料."
      },
      "limitParam": {
        "in": "query",
        "name": "limit",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 50,
          "default": 20
        },
        "description": "分頁參數 - 筆數 : 這參數表示要回傳幾筆資料."
      }
    },
    "schemas": {
      "JWE_REQUEST": {
        "title": "JWE 需求訊息",
        "type": "object",
        "description": "JWE request message.",
        "properties": {
          "enc_data": {
            "type": "string",
            "description": "JWE 加密訊息",
            "example": "eyJlbmMiOiJBMjU2R0NNIiwiYWxnIjoiZGlyIn0..OfzsKnjwT_kERUIZ.8FxP908nSr6ANbZa.meckSn-YS01yg_2AWxANoA"
          }
        }
      },
      "JWE_RESPONSE": {
        "title": "JWE 回應訊息",
        "type": "string",
        "description": "JWE response message.",
        "example": "eyJlbmMiOiJBMjU2R0NNIiwiYWxnIjoiZGlyIn0..OfzsKnjwT_kERUIZ.8FxP908nSr6ANbZa.meckSn-YS01yg_2AWxANoA"
      },
      "ERROR_RESPONSE": {
        "title": "錯誤回傳訊息",
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "example": "5001",
            "description": "處理結果代碼,非0表示異常"
          },
          "message": {
            "type": "string",
            "example": "自表檢核失敗",
            "description": "處理結果訊息"
          }
        }
      },
      "DECL_FORM_STATUS_GET_RESPONSE_PAYLOAD": {
        "title": "查詢報表申報狀態的查詢結果 : /decl/form/status : GET_RESPONSE_PAYLOAD",
        "type": "object",
        "properties": {
          "repBody": {
            "description": "回應訊息內容主體",
            "properties": {
              "forms": {
                "description": "查詢結果-報表資訊",
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/FORM_STATUS"
                }
              },
              "paging": {
                "$ref": "#/components/schemas/PAGING_INFO"
              }
            }
          }
        }
      },
      "DECL_FORM_APPLY_GET_RESPONSE_PAYLOAD": {
        "title": "查詢報表資料的查詢結果 : GET_RESPONSE_PAYLOAD",
        "type": "object",
        "properties": {
          "repBody": {
            "description": "回應訊息內容主體",
            "properties": {
              "forms": {
                "type": "object",
                "properties": {
                  "apply_info": {
                    "$ref": "#/components/schemas/APPLY_INFO_FOR_RESPONSE"
                  },
                  "form_data": {
                    "$ref": "#/components/schemas/FORM_DATA_FOR_RESPONSE"
                  }
                }
              }
            }
          }
        }
      },
      "DECL_FORM_APPLY_POST_REQUEST_PAYLOAD": {
        "title": "上傳報表資料的需求內容 : POST_REQUEST_PAYLOAD",
        "type": "object",
        "properties": {
          "postBody": {
            "description": "上傳內容",
            "type": "object",
            "properties": {
              "form": {
                "description": "報表內容",
                "type": "object",
                "properties": {
                  "apply_info": {
                    "$ref": "#/components/schemas/APPLY_INFO_FOR_REQUEST"
                  },
                  "form_data": {
                    "$ref": "#/components/schemas/FORM_DATA_FOR_REQUEST"
                  }
                }
              }
            }
          }
        }
      },
      "DECL_FORM_APPLY_POST_RESPONSE_PAYLOAD": {
        "title": "上傳報表資料的處理結果 : POST_RESPONSE_PAYLOAD",
        "type": "object",
        "properties": {
          "repBody": {
            "$ref": "#/components/schemas/FORM_DATA_FOR_APPLY_RESPONSE"
          }
        }
